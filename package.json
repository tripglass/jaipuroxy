  "main": "main.ts",
  "scripts": {
    "dev": "chcp 65001 && set NODE_DEBUG=module && set PINO_LOG_LEVEL=debug && ts-node src/main.ts | npx pino-pretty --levelFirst",
    "build": "tsoa spec-and-routes && tsc",
    "postbuild": "copyfiles -u 1 src/public/* dist/",
    "tsoa:routes": "tsoa routes --config tsoa.json",
    "tsoa:spec": "tsoa spec --config tsoa.json",
    "tsoa:gen": "npm run tsoa:routes && npm run tsoa:spec",
    "start": "ts-node src/main.ts",
    "start-prod": "node dist/main.js",
    "test": "vitest"
  },
