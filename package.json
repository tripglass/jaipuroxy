    "express": "^5.1.0",
    "pino": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "serve-favicon": "^2.5.1"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^22.0.2",
    "@tsconfig/node-ts": "^23.6.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.10.0",
    "@types/serve-favicon": "^2.5.7",
    "copyfiles": "^2.4.1",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "tsoa": "^6.6.0",
